<%= error_messages_for 'license_version' %>

<% form_for [@license, @license_version], :builder => TabularFormBuilder, :html => {:multipart => true} do |f| %>

  <%= render :partial => 'fields', :locals => {:f => f, :license => @license } %>

  <%= submit_tag l(@license.new_record? ? :button_create : :button_save) %>

<% end %>

